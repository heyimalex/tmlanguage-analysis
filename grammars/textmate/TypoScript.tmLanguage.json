        {
          "include": "#typo_defined_properties"
        },
        {
          "include": "#typo_constant"
        },
        {
          "include": "text.html.basic"
        }
      ]
    },
    {
      "begin": "\\(",
      "comment": "multiline value",
      "end": "\\)",
      "name": "support.other.multiline",
      "patterns": [
        {
          "include": "#typo_content_objects"
