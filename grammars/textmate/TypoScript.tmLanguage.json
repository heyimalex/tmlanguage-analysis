        "1": {
          "name": "support.function.delete.typoscript"
        }
      },
      "comment": "deleting/clearing of a typoscript object",
      "contentName": "support.function.delete.typoscript",
      "match": "^.[^=]*(>)",
      "name": "support.function.delete.typoscript"
    },
    {
      "begin": "(^[ \\t]+)?(?=#)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.whitespace.comment.leading.typoscript"
        }
      },
      "end": "(?!\\G)",
      "patterns": [
        {
          "begin": "#",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.typoscript"
            }
          },
          "end": "\\n",
          "name": "comment.line.number-sign.typoscript"
        }
      ]
    },
    {
      "begin": "(^[ \\t]*)(?=//)",
      "beginCaptures": {
        "1": {
          "name": "punctuation.whitespace.comment.leading.typoscript"
        }
      },
      "end": "(?!\\G)",
      "patterns": [
        {
          "begin": "//",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.typoscript"
            }
          },
          "end": "\\n",
          "name": "comment.line.double-slash.typoscript"
        }
      ]
    },
    {
      "begin": "/\\*",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.comment.begin.typoscript"
        }
      },
      "comment": "The end here is odd because the entire line with */ is a comment, even if after the closing punctuation.",
      "end": "(\\*/).*",
      "endCaptures": {
        "1": {
          "name": "punctuation.definition.comment.end.typoscript"
        }
      },
      "name": "comment.block.typoscript"
    },
    {
      "begin": "(<\\s+)",
      "beginCaptures": {
        "1": {
          "name": "keyword.operator.typoscript"
        }
      },
      "comment": "one typoscript object copied to another",
      "end": "(.+$)",
      "endCaptures": {
        "1": {
          "name": "support.function.copy.typoscript"
        }
      },
      "patterns": [
        {
          "include": "#typo_top_level_objects"
        },
        {
          "include": "#typo_stdWrap_all"
        },
        {
          "include": "#typo_defined_properties"
        }
      ]
    },
    {
      "begin": "=",
      "beginCaptures": {
        "0": {
          "name": "keyword.operator"
        }
      },
      "comment": "everything after the first equal sign",
      "end": "($)",
      "name": "string.interpolated.typoscript",
      "patterns": [
        {
          "include": "#typo_content_objects"
